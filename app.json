{
  "expo": {
    "name": "Lagrange",
    "slug": "Lagrange-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "hidden": true
    },
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "splash": {
        "backgroundColor": "#1672EC",
        "resizeMode": "contain",
        "image": "./assets/images/splash.png",
        "dark": {
          "backgroundColor": "#151718",
          "resizeMode": "contain",
          "image": "./assets/images/splash.png"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.expo_rn.Lagrangern"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ff8d303a-0a80-4dcc-a04e-6ecb5fbaa7ee"
      }
    }
  }
}
